{
    "ssh_port": "__ssh_port__",
    "zookeeper": {
        "node_ip": [
            "___node_ip___"
        ],
        "properties": {
            "storage": "/data/zookeeper-storage"
        },
        "java_version": "17",
        "python_version": "3.12.0"
    },
    "kraft_controller": {
        "properties": {
            "storage": "/data/kraft-controller-storage",            
        "ports": {
            "listener": 9093
        }
        }
    },
    
    "kafka": {
        "broker_ip": [
            "___node_ip___"
        ],
        "properties": {
            "storage": "/data/kafka-storage"
        },
        "java_version": "17",
        "python_version": "3.12.0"
    },
    "schema_registry": {
        "node_ip": [
            "___node_ip___"
        ]
    },
    "nifi": {
        "node_ip": [
            "___node_ip___"
        ],
        "java_version": "21",
        "python_version": "3.12.0"
    },
    "doris_fe": {
        "node_ip": [
            "___node_ip___"
        ],
        "properties": {
            "log": "${DORIS_HOME}/log"
        },
        "java_version": "17",
        "python_version": "3.12.0"
    },
    "doris_be": {
        "node_ip": [
            "__doris_be_ip__"
        ],
        "properties": {
            "log": "${DORIS_HOME}/log",
            "storage": "/data/doris-be-storage"
        },
        "java_version": "17",
        "python_version": "3.12.0"
    },
    "monitoring": {
        "node_ip": "___node_ip___",
        "properties": {
            "storage": "/data/grafana-storage"
        },
        "java_version": "17",
