    "api": {
        "node_ip": [
            "___node_ip___"
        ],
        "java_version": "17",
        "python_version": "3.12.0"
    },
    
    "backend_job": {
        "node_ip": [
            "___node_ip___"
    ],
        "java_version": "17",
        "python_version": "3.12.0"
    },
    "nginx": {
        "node_ip": "___node_ip___"
  }
}